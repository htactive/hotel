@{
    ViewData["Title"] = "Service details";
}
<section class="sub_header" id="bg_general">
    <div class="sub_header_content">
        <div class="animated fadeInDown">
            <h1>@Model?.Service?.Title</h1>
            <p>
                @Html.Raw(Model?.Service.ShortDescription)
            </p>
        </div>
    </div>
</section><!-- End Section -->

<div class="container margin_60">
    <div class="row">
        <div class="col-lg-12 col-md-12">
            <img src="@(Model?.Service?.CoverImage?.Url ?? "/img/food_1.jpg")" alt="" class="img-responsive styled nopadding">
            <p>
                @Html.Raw(Model?.Service?.Html)
            </p>

        </div><!-- End col-lg-9 -->
    </div><!-- End row -->
</div><!-- End Container -->